{
  "version": 8,
  "sources": {
    "my_geojson": {
      "type": "geojson",
      "data": "https://raw.githubusercontent.com/mks-mir/geoconfig_repo/main/geojson.json"
    }
  },
  "layers": [
    {
      "id": "my_geojson_fill",
      "type": "fill",
      "source": "my_geojson",
      "paint": {
        "fill-color": "#4F7CAC",
        "fill-opacity": 0.4
      }
    },
    {
      "id": "my_geojson_outline",
      "type": "line",
      "source": "my_geojson",
      "paint": {
        "line-color": "#2E4057",
        "line-width": 1
      }
    }
  ],
  "glyphs": "https://demotiles.maplibre.org/font/{fontstack}/{range}.pbf",
  "sprite": "https://demotiles.maplibre.org/styles/osm-bright/sprite"
}
